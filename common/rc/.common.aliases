# some more ls aliases
alias ll='ls -alhF'
alias la='ls -Alh'
alias l='ls -CF'

alias mkvenv="python3 -m venv venv"
alias vac="source venv/bin/activate"
alias dac="deactivate"

# Git aliases
alias gco="git checkout"
alias gci="git commit"
alias gps="git push"
alias gpl="git pull"
alias ga="git add"
alias gs="git status"
alias glog="git log --graph --all --decorate --oneline"
